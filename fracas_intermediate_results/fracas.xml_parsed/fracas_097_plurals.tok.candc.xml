<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Software" lemma="Software" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="faults" lemma="fault" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="were" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
    <rule type="fa" cat="S[pss]\NP">
     <lf start="3" span="1" word="blamed" lemma="blame" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="4" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="6" span="1" word="system" lemma="system" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="7" span="1" word="failure" lemma="failure" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="system" lemma="system" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="failure" lemma="failure" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
    <rule type="fa" cat="S[pss]\NP">
     <lf start="4" span="1" word="blamed" lemma="blame" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="5" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <rule type="ba" cat="N/N">
         <lf start="6" span="1" word="one" lemma="one" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
         <rule type="conj" cat="(N/N)\(N/N)">
          <lf start="7" span="1" word="or" lemma="or" pos="CC" chunk="I-NP" entity="O" cat="conj" />
          <lf start="8" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="N/N" />
         </rule>
        </rule>
        <rule type="fa" cat="N">
         <lf start="9" span="1" word="software" lemma="software" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="10" span="1" word="faults" lemma="fault" pos="NNS" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
