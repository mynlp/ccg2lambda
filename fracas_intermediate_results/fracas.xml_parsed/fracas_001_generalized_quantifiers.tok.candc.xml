<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <rule type="fa" cat="S[adj]\NP">
   <lf start="0" span="1" word="An" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="(S[adj]\NP)/(S[adj]\NP)" />
   <lf start="1" span="1" word="Italian" lemma="italian" pos="JJ" chunk="I-NP" entity="O" cat="S[adj]\NP" />
  </rule>
  <rule type="rp" cat="S[dcl]\(S[adj]\NP)">
   <rule type="fa" cat="S[dcl]\(S[adj]\NP)">
    <lf start="2" span="1" word="became" lemma="become" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\(S[adj]\NP))/NP" />
    <rule type="fa" cat="NP[nb]">
     <rule type="ba" cat="NP[nb]/N">
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="4" span="1" word="world" lemma="world" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <lf start="5" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
     </rule>
     <rule type="fa" cat="N">
      <lf start="6" span="1" word="greatest" lemma="greatest" pos="JJS" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="7" span="1" word="tenor" lemma="tenor" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="fa" cat="S[dcl]\NP[thr]">
    <lf start="1" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="Italian" lemma="italian" pos="JJ" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="who" lemma="who" pos="WP" chunk="I-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="5" span="1" word="became" lemma="become" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <rule type="ba" cat="NP[nb]/N">
         <rule type="fa" cat="NP[nb]">
          <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <lf start="7" span="1" word="world" lemma="world" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
         <lf start="8" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
        </rule>
        <rule type="fa" cat="N">
         <lf start="9" span="1" word="greatest" lemma="greatest" pos="JJS" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="10" span="1" word="tenor" lemma="tenor" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
