<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="1" span="1" word="took" lemma="take" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="3" span="1" word="machine" lemma="machine" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="4" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="5" span="1" word="Tuesday" lemma="Tuesday" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="conj" cat="S[dcl]\S[dcl]">
    <lf start="6" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="conj" cat="S[dcl]\S[dcl]">
     <lf start="7" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
     <rule type="ba" cat="S[dcl]">
      <rule type="lex" cat="NP">
       <lf start="8" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="9" span="1" word="took" lemma="take" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="10" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="11" span="1" word="machine" lemma="machine" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="12" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="lex" cat="NP">
          <lf start="13" span="1" word="Wednesday" lemma="Wednesday" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="14" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="They" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="fa" cat="(S[dcl]\NP)/PP">
     <lf start="1" span="1" word="put" lemma="put" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/PP)/NP" />
     <lf start="2" span="1" word="them" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
    </rule>
    <rule type="fa" cat="PP">
     <lf start="3" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="lobby" lemma="lobby" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="fc" cat="S[dcl]/PP">
  <rule type="tr" cat="S[X]/(S[X]\NP)">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
    <rule type="conj" cat="NP\NP">
     <lf start="1" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="I-PER" cat="conj" />
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="fa" cat="(S[dcl]\NP)/PP">
   <lf start="3" span="1" word="put" lemma="put" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/PP)/NP" />
   <rule type="rp" cat="NP">
    <rule type="ba" cat="NP">
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="4" span="1" word="two" lemma="two" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="5" span="1" word="machines" lemma="machine" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="8" span="1" word="lobby" lemma="lobby" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
