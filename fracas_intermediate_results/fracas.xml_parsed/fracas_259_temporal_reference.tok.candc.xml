<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="conference" lemma="conference" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <lf start="2" span="1" word="started" lemma="start" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="3" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="lex" cat="NP">
      <rule type="ba" cat="N">
       <rule type="rp" cat="N">
        <rule type="fa" cat="N">
         <lf start="4" span="1" word="July" lemma="July" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N/N[num]" />
         <lf start="5" span="1" word="4th" lemma="4th" pos="CD" chunk="I-NP" entity="I-DAT" cat="N[num]" />
        </rule>
        <lf start="6" span="1" word="," lemma="," pos="," chunk="I-NP" entity="I-DAT" cat="," />
       </rule>
       <lf start="7" span="1" word="1994" lemma="1994" pos="CD" chunk="I-NP" entity="I-DAT" cat="N\N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="It" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="lasted" lemma="last" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="2" lemma="2" pos="CD" chunk="I-NP" entity="I-DAT" cat="N/N" />
      <lf start="3" span="1" word="days" lemma="day" pos="NNS" chunk="I-NP" entity="I-DAT" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="conference" lemma="conference" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <lf start="3" span="1" word="over" lemma="over" pos="IN" chunk="I-PP" entity="O" cat="S[adj]\NP" />
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="4" span="1" word="on" lemma="on" pos="IN" chunk="B-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="lex" cat="NP">
      <rule type="ba" cat="N">
       <rule type="rp" cat="N">
        <rule type="fa" cat="N">
         <lf start="5" span="1" word="July" lemma="July" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N/N[num]" />
         <lf start="6" span="1" word="8th" lemma="8th" pos="CD" chunk="I-NP" entity="I-DAT" cat="N[num]" />
        </rule>
        <lf start="7" span="1" word="," lemma="," pos="," chunk="I-NP" entity="I-DAT" cat="," />
       </rule>
       <lf start="8" span="1" word="1994" lemma="1994" pos="CD" chunk="I-NP" entity="I-DAT" cat="N\N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
