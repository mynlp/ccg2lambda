<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <lf start="1" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="wrote" lemma="write" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="lp" cat="S[dcl]">
    <lf start="5" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="fa" cat="S[dcl]">
     <lf start="6" span="1" word="then" lemma="then" pos="RB" chunk="O" entity="O" cat="S/S" />
     <rule type="ba" cat="S[dcl]">
      <rule type="lex" cat="NP">
       <lf start="7" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
      </rule>
      <rule type="ba" cat="S[dcl]\NP">
       <lf start="8" span="1" word="did" lemma="do" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
       <lf start="9" span="1" word="too" lemma="too" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="wrote" lemma="write" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="3" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="wrote" lemma="write" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="3" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
