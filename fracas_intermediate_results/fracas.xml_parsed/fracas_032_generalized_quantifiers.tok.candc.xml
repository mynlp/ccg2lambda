<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[X]/S[X]">
  <lf start="0" span="1" word="At" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
  <rule type="rp" cat="NP">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="most" lemma="most" pos="JJS" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="ten" lemma="ten" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="commissioners" lemma="commissioner" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="lex" cat="NP\NP">
     <rule type="ba" cat="S[dcl]\NP">
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="4" span="1" word="spend" lemma="spend" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="6" span="1" word="lot" lemma="lot" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="7" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="lex" cat="NP">
          <lf start="8" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="9" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="lex" cat="NP">
        <lf start="10" span="1" word="home" lemma="home" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="fa" cat="S[X]/S[X]">
  <lf start="0" span="1" word="At" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
  <rule type="rp" cat="NP">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="most" lemma="most" pos="JJS" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="ten" lemma="ten" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="commissioners" lemma="commissioner" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="lex" cat="NP\NP">
     <rule type="ba" cat="S[dcl]\NP">
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="4" span="1" word="spend" lemma="spend" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="lex" cat="NP">
        <lf start="5" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="6" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="lex" cat="NP">
        <lf start="7" span="1" word="home" lemma="home" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
