<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="fa" cat="S[X]/S[X]">
    <lf start="0" span="1" word="When" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(S/S)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <lf start="1" span="1" word="they" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="2" span="1" word="opened" lemma="open" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="4" span="1" word="M25" lemma="M25" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="lp" cat="S[dcl]">
    <lf start="5" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="6" span="1" word="traffic" lemma="traffic" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <lf start="7" span="1" word="increased" lemma="increase" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
