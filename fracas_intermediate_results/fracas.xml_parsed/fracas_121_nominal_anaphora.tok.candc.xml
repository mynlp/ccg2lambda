<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="fa" cat="(S[dcl]\NP)/PP">
     <lf start="1" span="1" word="delivered" lemma="deliver" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/PP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="PP">
     <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="lex" cat="NP">
      <lf start="5" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="She" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="also" lemma="also" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
    <rule type="fa" cat="S[dcl]\NP">
     <rule type="fa" cat="(S[dcl]\NP)/NP">
      <lf start="2" span="1" word="delivered" lemma="deliver" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/NP)/NP" />
      <lf start="3" span="1" word="them" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     </rule>
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="an" lemma="an" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="invoice" lemma="invoice" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <lf start="0" span="1" word="And" lemma="and" pos="CC" chunk="O" entity="O" cat="S/S" />
   <rule type="ba" cat="S[dcl]">
    <lf start="1" span="1" word="she" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
    <rule type="fa" cat="S[dcl]\NP">
     <rule type="fa" cat="(S[dcl]\NP)/NP">
      <lf start="2" span="1" word="delivered" lemma="deliver" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/NP)/NP" />
      <lf start="3" span="1" word="them" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     </rule>
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="B-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="5" span="1" word="project" lemma="project" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="6" span="1" word="proposal" lemma="proposal" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="delivered" lemma="deliver" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="conj" cat="NP\NP">
      <lf start="4" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="an" lemma="an" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="6" span="1" word="invoice" lemma="invoice" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="conj" cat="NP\NP">
        <lf start="7" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
        <rule type="ba" cat="NP">
         <rule type="fa" cat="NP[nb]">
          <lf start="8" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="9" span="1" word="project" lemma="project" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="10" span="1" word="proposal" lemma="proposal" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
         <rule type="fa" cat="NP\NP">
          <lf start="11" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
          <rule type="lex" cat="NP">
           <lf start="12" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="13" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
