<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="wants" lemma="want" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[to]\NP)" />
    <rule type="fa" cat="S[to]\NP">
     <lf start="2" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
     <rule type="fa" cat="S[b]\NP">
      <lf start="3" span="1" word="know" lemma="know" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/S[qem]" />
      <rule type="fa" cat="S[qem]">
       <rule type="fa" cat="S[qem]/(S[dcl]\NP)">
        <rule type="fa" cat="(S[qem]/(S[dcl]\NP))/N">
         <lf start="4" span="1" word="how" lemma="how" pos="WRB" chunk="I-NP" entity="O" cat="((S[qem]/(S[dcl]\NP))/N)/(S[adj]\NP)" />
         <lf start="5" span="1" word="many" lemma="many" pos="JJ" chunk="I-NP" entity="O" cat="S[adj]\NP" />
        </rule>
        <lf start="6" span="1" word="men" lemma="man" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="7" span="1" word="work" lemma="work" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="8" span="1" word="part" lemma="part" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="9" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[frg]">
  <rule type="fa" cat="S[frg]">
   <lf start="0" span="1" word="And" lemma="and" pos="CC" chunk="O" entity="O" cat="S[frg]/NP" />
   <rule type="lex" cat="NP">
    <lf start="1" span="1" word="women" lemma="woman" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
  </rule>
  <lf start="2" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="wants" lemma="want" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[to]\NP)" />
    <rule type="fa" cat="S[to]\NP">
     <lf start="2" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
     <rule type="fa" cat="S[b]\NP">
      <lf start="3" span="1" word="know" lemma="know" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/S[qem]" />
      <rule type="fa" cat="S[qem]">
       <rule type="fa" cat="S[qem]/(S[dcl]\NP)">
        <rule type="fa" cat="(S[qem]/(S[dcl]\NP))/N">
         <lf start="4" span="1" word="how" lemma="how" pos="WRB" chunk="I-NP" entity="O" cat="((S[qem]/(S[dcl]\NP))/N)/(S[adj]\NP)" />
         <lf start="5" span="1" word="many" lemma="many" pos="JJ" chunk="I-NP" entity="O" cat="S[adj]\NP" />
        </rule>
        <lf start="6" span="1" word="women" lemma="woman" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="7" span="1" word="work" lemma="work" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="8" span="1" word="part" lemma="part" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="9" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
