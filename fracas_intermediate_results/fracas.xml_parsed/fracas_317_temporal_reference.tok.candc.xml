<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="representative" lemma="representative" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
    <rule type="fa" cat="S[pt]\NP">
     <lf start="3" span="1" word="read" lemma="read" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="this" lemma="this" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="No" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="two" lemma="two" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="representatives" lemma="representative" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="have" lemma="have" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
    <rule type="ba" cat="S[pt]\NP">
     <rule type="fa" cat="S[pt]\NP">
      <lf start="4" span="1" word="read" lemma="read" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/NP" />
      <lf start="5" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="8" span="1" word="same" lemma="same" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="9" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="No" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="representative" lemma="representative" pos="NN" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="took" lemma="take" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="fa" cat="NP">
      <rule type="fa" cat="NP/NP">
       <rule type="ba" cat="(NP/NP)/(NP/NP)">
        <lf start="3" span="1" word="less" lemma="less" pos="JJR" chunk="I-NP" entity="O" cat="S[adj]\NP" />
        <lf start="4" span="1" word="than" lemma="than" pos="IN" chunk="I-NP" entity="O" cat="((NP/NP)/(NP/NP))\(S[adj]\NP)" />
       </rule>
       <lf start="5" span="1" word="half" lemma="half" pos="PDT" chunk="I-NP" entity="O" cat="NP/NP" />
      </rule>
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="7" span="1" word="day" lemma="day" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
     <rule type="fa" cat="S[to]\NP">
      <lf start="8" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
      <rule type="fa" cat="S[b]\NP">
       <lf start="9" span="1" word="read" lemma="read" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="11" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="12" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
   <rule type="fa" cat="S[dcl]\NP[thr]">
    <lf start="1" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="sixteen" lemma="sixteen" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="3" span="1" word="representatives" lemma="representative" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="It" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="1" span="1" word="took" lemma="take" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="3" span="1" word="representatives" lemma="representative" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="ba" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="4" span="1" word="more" lemma="more" pos="RBR" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
      <rule type="fa" cat="((S[X]\NP)\(S[X]\NP))\((S[X]\NP)\(S[X]\NP))">
       <lf start="5" span="1" word="than" lemma="than" pos="IN" chunk="I-PP" entity="O" cat="(((S\NP)\(S\NP))\((S\NP)\(S\NP)))/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="I-DAT" cat="NP[nb]/N" />
        <lf start="7" span="1" word="week" lemma="week" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
     <rule type="fa" cat="S[to]\NP">
      <lf start="8" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
      <rule type="fa" cat="S[b]\NP">
       <lf start="9" span="1" word="read" lemma="read" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="11" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="12" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
