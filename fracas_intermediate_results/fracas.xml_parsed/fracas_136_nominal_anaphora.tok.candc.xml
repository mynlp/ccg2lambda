<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="Every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="executive" lemma="executive" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="who" lemma="who" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="had" lemma="have" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="laptop" lemma="laptop" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="6" span="1" word="computer" lemma="computer" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="fa" cat="(S[dcl]\NP)/(S[to]\NP)">
     <lf start="7" span="1" word="brought" lemma="bring" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/(S[to]\NP))/NP" />
     <lf start="8" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
    </rule>
    <rule type="fa" cat="S[to]\NP">
     <lf start="9" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
     <rule type="ba" cat="S[b]\NP">
      <rule type="fa" cat="S[b]\NP">
       <lf start="10" span="1" word="take" lemma="take" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
       <rule type="lex" cat="NP">
        <lf start="11" span="1" word="notes" lemma="note" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="12" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="13" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="14" span="1" word="meeting" lemma="meeting" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="15" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="executive" lemma="executive" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="who" lemma="who" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="5" span="1" word="owns" lemma="own" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="6" span="1" word="five" lemma="five" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
         <rule type="fa" cat="N">
          <lf start="7" span="1" word="different" lemma="different" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <rule type="fa" cat="N">
           <lf start="8" span="1" word="laptop" lemma="laptop" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="9" span="1" word="computers" lemma="computer" pos="NNS" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="fa" cat="(S[dcl]\NP)/PP">
     <lf start="1" span="1" word="took" lemma="take" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/PP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="five" lemma="five" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="3" span="1" word="laptop" lemma="laptop" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="4" span="1" word="computers" lemma="computer" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="PP">
     <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="7" span="1" word="meeting" lemma="meeting" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
