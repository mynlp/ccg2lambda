<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="ba" cat="S[dcl]\NP">
      <lf start="1" span="1" word="went" lemma="go" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="2" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="lex" cat="NP">
        <lf start="3" span="1" word="Paris" lemma="Paris" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="4" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <lf start="5" span="1" word="car" lemma="car" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="conj" cat="NP\NP">
        <lf start="6" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
        <rule type="conj" cat="NP\NP">
         <lf start="7" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
         <rule type="lex" cat="NP">
          <lf start="8" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="9" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="lex" cat="NP">
      <lf start="10" span="1" word="Berlin" lemma="Berlin" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="ba" cat="S[dcl]\NP">
     <lf start="1" span="1" word="went" lemma="go" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="2" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="3" span="1" word="Berlin" lemma="Berlin" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="4" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="lex" cat="NP">
      <lf start="5" span="1" word="car" lemma="car" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
