<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="had" lemma="have" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
    <rule type="ba" cat="S[pt]\NP">
     <rule type="ba" cat="S[pt]\NP">
      <rule type="fa" cat="S[pt]\NP">
       <lf start="2" span="1" word="left" lemma="leave" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="4" span="1" word="house" lemma="house" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="5" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="7" span="1" word="quarter" lemma="quarter" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="8" span="1" word="past" lemma="past" pos="JJ" chunk="I-ADJP" entity="I-DAT" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="9" span="1" word="five" lemma="five" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <lf start="0" span="1" word="Then" lemma="then" pos="RB" chunk="I-ADVP" entity="O" cat="S/S" />
   <rule type="ba" cat="S[dcl]">
    <lf start="1" span="1" word="she" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="2" span="1" word="took" lemma="take" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="4" span="1" word="taxi" lemma="taxi" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="7" span="1" word="station" lemma="station" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="1" span="1" word="left" lemma="leave" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="house" lemma="house" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="4" span="1" word="before" lemma="before" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
     <rule type="ba" cat="S[dcl]">
      <lf start="5" span="1" word="she" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
      <rule type="ba" cat="S[dcl]\NP">
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="6" span="1" word="took" lemma="take" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="8" span="1" word="taxi" lemma="taxi" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="9" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="11" span="1" word="station" lemma="station" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="12" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
