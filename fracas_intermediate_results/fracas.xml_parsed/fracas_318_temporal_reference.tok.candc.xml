<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="fa" cat="S[X]/S[X]">
    <lf start="0" span="1" word="While" lemma="while" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="1" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="2" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
      <rule type="fa" cat="S[ng]\NP">
       <lf start="3" span="1" word="updating" lemma="update" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="5" span="1" word="program" lemma="program" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="lp" cat="S[dcl]">
    <lf start="6" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="7" span="1" word="Mary" lemma="Mary" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
     </rule>
     <rule type="ba" cat="S[dcl]\NP">
      <rule type="ba" cat="S[dcl]\NP">
       <lf start="8" span="1" word="came" lemma="come" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
       <lf start="9" span="1" word="in" lemma="in" pos="IN" chunk="I-PRT" entity="O" cat="(S\NP)\(S\NP)" />
      </rule>
      <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
       <lf start="10" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="fa" cat="S[dcl]\NP">
        <rule type="fa" cat="(S[dcl]\NP)/PP">
         <lf start="11" span="1" word="told" lemma="tell" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/PP)/NP" />
         <lf start="12" span="1" word="him" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
        </rule>
        <rule type="fa" cat="PP">
         <lf start="13" span="1" word="about" lemma="about" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
         <rule type="fa" cat="NP[nb]">
          <lf start="14" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
          <rule type="fa" cat="N">
           <lf start="15" span="1" word="board" lemma="board" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="16" span="1" word="meeting" lemma="meeting" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="17" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="She" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
   <rule type="ba" cat="S[dcl]\NP">
    <lf start="1" span="1" word="finished" lemma="finish" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="2" span="1" word="before" lemma="before" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
     <rule type="ba" cat="S[dcl]">
      <lf start="3" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
      <lf start="4" span="1" word="did" lemma="do" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Mary" lemma="Mary" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="N/N" />
      <lf start="2" span="1" word="story" lemma="story" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="3" span="1" word="lasted" lemma="last" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
     <rule type="fa" cat="S[adj]\NP">
      <lf start="4" span="1" word="as" lemma="as" pos="RB" chunk="I-ADVP" entity="O" cat="(S[adj]\NP)/(S[adj]\NP)" />
      <rule type="fa" cat="S[adj]\NP">
       <lf start="5" span="1" word="long" lemma="long" pos="JJ" chunk="I-ADVP" entity="O" cat="(S[adj]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="6" span="1" word="as" lemma="as" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <lf start="7" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
          <lf start="8" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
     <rule type="fa" cat="S[ng]\NP">
      <lf start="9" span="1" word="updating" lemma="update" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="10" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="11" span="1" word="program" lemma="program" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="12" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
