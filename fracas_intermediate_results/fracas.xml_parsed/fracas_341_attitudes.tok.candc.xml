<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="fa" cat="(S[dcl]\NP)/(S[b]\NP)">
     <lf start="1" span="1" word="saw" lemma="see" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/(S[b]\NP))/NP" />
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[b]\NP">
     <lf start="3" span="1" word="sign" lemma="sign" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="contract" lemma="contract" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="When" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <lf start="1" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="signed" lemma="sign" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="contract" lemma="contract" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="lp" cat="S[dcl]">
    <lf start="5" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
    <rule type="ba" cat="S[dcl]">
     <rule type="fa" cat="NP[nb]">
      <lf start="6" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="7" span="1" word="heart" lemma="heart" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="8" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
      <lf start="9" span="1" word="beating" lemma="beat" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
     </rule>
    </rule>
   </rule>
   <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]/NP">
  <rule type="fc" cat="S[dcl]/NP">
   <rule type="tr" cat="S[X]/(S[X]\NP)">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
   </rule>
   <rule type="fc" cat="(S[dcl]\NP)/NP">
    <lf start="1" span="1" word="saw" lemma="see" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
    <rule type="fc" cat="S[dcl]/NP">
     <rule type="tr" cat="S[X]/(S[X]\NP)">
      <rule type="fa" cat="NP[nb]">
       <rule type="ba" cat="NP[nb]/N">
        <rule type="lex" cat="NP">
         <lf start="2" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
        </rule>
        <lf start="3" span="1" word="'" lemma="'" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
       </rule>
       <lf start="4" span="1" word="heart" lemma="heart" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <lf start="5" span="1" word="beat" lemma="beat" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
