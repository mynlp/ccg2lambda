<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="When" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <lf start="1" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="got" lemma="get" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="4" span="1" word="job" lemma="job" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="5" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="6" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="7" span="1" word="CIA" lemma="CIA" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="lp" cat="S[dcl]">
   <lf start="8" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <lf start="9" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="10" span="1" word="knew" lemma="know" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[em]" />
      <rule type="fa" cat="S[em]">
       <lf start="11" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
       <rule type="ba" cat="S[dcl]">
        <lf start="12" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
        <rule type="fa" cat="S[dcl]\NP">
         <rule type="bx" cat="(S[dcl]\NP)/(S[b]\NP)">
          <lf start="13" span="1" word="would" lemma="would" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
          <lf start="14" span="1" word="never" lemma="never" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)\(S\NP)" />
         </rule>
         <rule type="fa" cat="S[b]\NP">
          <lf start="15" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[pss]\NP)" />
          <rule type="fa" cat="S[pss]\NP">
           <lf start="16" span="1" word="allowed" lemma="allow" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/(S[to]\NP)" />
           <rule type="fa" cat="S[to]\NP">
            <lf start="17" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
            <rule type="fa" cat="S[b]\NP">
             <lf start="18" span="1" word="write" lemma="write" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
             <rule type="fa" cat="NP[nb]">
              <lf start="19" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
              <lf start="20" span="1" word="memoirs" lemma="memoirs" pos="NNS" chunk="I-NP" entity="O" cat="N" />
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="21" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="It" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP[expl]" />
   <rule type="fa" cat="S[dcl]\NP[expl]">
    <rule type="fa" cat="(S[dcl]\NP[expl])/S[em]">
     <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="((S[dcl]\NP[expl])/S[em])/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="case" lemma="case" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="S[em]">
     <lf start="4" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
     <rule type="ba" cat="S[dcl]">
      <rule type="lex" cat="NP">
       <lf start="5" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
      <rule type="fa" cat="S[dcl]\NP">
       <rule type="ba" cat="(S[dcl]\NP)/NP">
        <rule type="bx" cat="(S[dcl]\NP)/NP">
         <lf start="6" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
         <lf start="7" span="1" word="not" lemma="not" pos="RB" chunk="O" entity="O" cat="(S\NP)\(S\NP)" />
        </rule>
        <rule type="conj" cat="((S[dcl]\NP)/NP)\((S[dcl]\NP)/NP)">
         <lf start="8" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
         <rule type="fc" cat="(S[dcl]\NP)/NP">
          <rule type="bx" cat="(S[dcl]\NP)/(S[b]\NP)">
           <lf start="9" span="1" word="will" lemma="will" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
           <lf start="10" span="1" word="never" lemma="never" pos="RB" chunk="I-VP" entity="O" cat="(S\NP)\(S\NP)" />
          </rule>
          <rule type="fc" cat="(S[b]\NP)/NP">
           <lf start="11" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[pss]\NP)" />
           <rule type="fc" cat="(S[pss]\NP)/NP">
            <lf start="12" span="1" word="allowed" lemma="allow" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/(S[to]\NP)" />
            <rule type="fc" cat="(S[to]\NP)/NP">
             <lf start="13" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
             <lf start="14" span="1" word="write" lemma="write" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
       <rule type="fa" cat="NP[nb]">
        <lf start="15" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="16" span="1" word="memoirs" lemma="memoirs" pos="NNS" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="17" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
