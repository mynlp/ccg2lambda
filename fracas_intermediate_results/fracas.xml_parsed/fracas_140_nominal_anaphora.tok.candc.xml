<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="said" lemma="say" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="had" lemma="have" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
      <rule type="fa" cat="S[pt]\NP">
       <lf start="4" span="1" word="hurt" lemma="hurt" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/NP" />
       <lf start="5" span="1" word="himself" lemma="himself" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="said" lemma="say" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="had" lemma="have" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
      <rule type="fa" cat="S[pt]\NP">
       <lf start="4" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[pss]\NP)" />
       <lf start="5" span="1" word="hurt" lemma="hurt" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
