<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="Canadian" lemma="canadian" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="resident" lemma="resident" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="can" lemma="can" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
    <rule type="ba" cat="S[b]\NP">
     <rule type="ba" cat="S[b]\NP">
      <lf start="4" span="1" word="travel" lemma="travel" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
      <lf start="5" span="1" word="freely" lemma="freely" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="within" lemma="within" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="7" span="1" word="Europe" lemma="Europe" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="Canadian" lemma="canadian" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="resident" lemma="resident" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="5" span="1" word="resident" lemma="resident" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="6" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="8" span="1" word="North" lemma="North" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="9" span="1" word="American" lemma="American" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N/N" />
         <lf start="10" span="1" word="continent" lemma="continent" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="Every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="resident" lemma="resident" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <rule type="fa" cat="N/N">
        <lf start="4" span="1" word="North" lemma="north" pos="JJ" chunk="I-NP" entity="O" cat="(N/N)/(N/N)" />
        <lf start="5" span="1" word="American" lemma="american" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       </rule>
       <lf start="6" span="1" word="continent" lemma="continent" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="7" span="1" word="can" lemma="can" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
    <rule type="ba" cat="S[b]\NP">
     <rule type="ba" cat="S[b]\NP">
      <lf start="8" span="1" word="travel" lemma="travel" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
      <lf start="9" span="1" word="freely" lemma="freely" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="10" span="1" word="within" lemma="within" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="11" span="1" word="Europe" lemma="Europe" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="12" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
