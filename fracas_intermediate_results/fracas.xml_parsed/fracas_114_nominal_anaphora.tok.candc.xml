<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="N">
  <lf start="0" span="1" word="Mary" lemma="Mary" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
  <rule type="rp" cat="N">
   <rule type="fa" cat="N">
    <lf start="1" span="1" word="used" lemma="use" pos="VBD" chunk="I-VP" entity="O" cat="N/N" />
    <rule type="fa" cat="N">
     <lf start="2" span="1" word="her" lemma="her" pos="PRP$" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="3" span="1" word="workstation" lemma="workstation" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="0" span="1" word="Mary" lemma="Mary" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
      <lf start="1" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <lf start="2" span="1" word="workstation" lemma="workstation" pos="NN" chunk="I-NP" entity="O" cat="NP\NP" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
    <lf start="4" span="1" word="used" lemma="use" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
