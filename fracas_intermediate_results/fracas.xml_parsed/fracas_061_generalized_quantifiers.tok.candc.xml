<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="NP">
  <rule type="ba" cat="NP">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Both" lemma="both" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="female" lemma="female" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="commissioners" lemma="commissioner" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="lex" cat="NP\NP">
    <rule type="fa" cat="S[pss]\NP">
     <lf start="3" span="1" word="used" lemma="use" pos="VBN" chunk="I-VP" entity="O" cat="(S[pss]\NP)/(S[to]\NP)" />
     <rule type="fa" cat="S[to]\NP">
      <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
      <rule type="fa" cat="S[b]\NP">
       <lf start="5" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
        <rule type="lex" cat="NP">
         <lf start="7" span="1" word="business" lemma="business" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Both" lemma="both" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="commissioners" lemma="commissioner" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="used" lemma="use" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[to]\NP)" />
    <rule type="fa" cat="S[to]\NP">
     <lf start="3" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
     <rule type="fa" cat="S[b]\NP">
      <lf start="4" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/PP" />
      <rule type="fa" cat="PP">
       <lf start="5" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="lex" cat="NP">
        <lf start="6" span="1" word="business" lemma="business" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
