<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="N">
  <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
  <rule type="rp" cat="N">
   <lf start="1" span="1" word="swam" lemma="swam" pos="NN" chunk="I-NP" entity="O" cat="N" />
   <lf start="2" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="fa" cat="N">
  <lf start="0" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
  <rule type="rp" cat="N">
   <lf start="1" span="1" word="swam" lemma="swam" pos="NN" chunk="I-NP" entity="O" cat="N" />
   <lf start="2" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="NP">
  <rule type="ba" cat="NP">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
     <lf start="1" span="1" word="swam" lemma="swam" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="NP\NP">
    <lf start="2" span="1" word="before" lemma="before" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
      <lf start="4" span="1" word="swam" lemma="swam" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="NP">
  <rule type="ba" cat="NP">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N/N" />
     <lf start="1" span="1" word="swam" lemma="swam" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="NP\NP">
    <lf start="2" span="1" word="after" lemma="after" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N/N" />
      <lf start="4" span="1" word="swam" lemma="swam" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
