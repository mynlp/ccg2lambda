<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Five" lemma="five" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="1" span="1" word="men" lemma="man" pos="NNS" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="work" lemma="work" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="part" lemma="part" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="4" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="fc" cat="S[X]/S[X]">
  <lf start="0" span="1" word="And" lemma="and" pos="CC" chunk="O" entity="O" cat="S/S" />
  <rule type="rp" cat="S[X]/S[X]">
   <rule type="lex" cat="S[X]/S[X]">
    <rule type="fa" cat="S[adj]\NP">
     <lf start="1" span="1" word="forty" lemma="forty" pos="JJ" chunk="I-NP" entity="O" cat="(S[adj]\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="2" span="1" word="five" lemma="five" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="3" span="1" word="women" lemma="woman" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <rule type="fa" cat="N">
     <lf start="0" span="1" word="Forty" lemma="Forty" pos="NNP" chunk="I-NP" entity="O" cat="N/N" />
     <rule type="fa" cat="N">
      <lf start="1" span="1" word="five" lemma="five" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="2" span="1" word="women" lemma="woman" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="work" lemma="work" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="4" span="1" word="part" lemma="part" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="5" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
