<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="ba" cat="S[ng]\NP">
     <rule type="fa" cat="S[ng]\NP">
      <lf start="2" span="1" word="running" lemma="run" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="4" span="1" word="own" lemma="own" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="5" span="1" word="business" lemma="business" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <rule type="fa" cat="N">
        <lf start="7" span="1" word="two" lemma="two" pos="CD" chunk="I-NP" entity="I-DAT" cat="N/N" />
        <lf start="8" span="1" word="years" lemma="year" pos="NNS" chunk="I-NP" entity="I-DAT" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="1" span="1" word="spent" lemma="spend" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <rule type="fa" cat="N/N">
        <rule type="ba" cat="(N/N)/(N/N)">
         <lf start="2" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="S[adj]\NP" />
         <lf start="3" span="1" word="than" lemma="than" pos="IN" chunk="I-NP" entity="O" cat="((N/N)/(N/N))\(S[adj]\NP)" />
        </rule>
        <lf start="4" span="1" word="two" lemma="two" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
       </rule>
       <lf start="5" span="1" word="years" lemma="year" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="lex" cat="(S[X]\NP)\(S[X]\NP)">
     <rule type="fa" cat="S[ng]\NP">
      <lf start="6" span="1" word="running" lemma="run" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="7" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="8" span="1" word="own" lemma="own" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="9" span="1" word="business" lemma="business" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
