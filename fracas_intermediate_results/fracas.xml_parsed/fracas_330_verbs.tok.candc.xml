<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="1" span="1" word="owned" lemma="own" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="APCOM" lemma="APCOM" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <rule type="fa" cat="((S[X]\NP)\(S[X]\NP))/((S[X]\NP)\(S[X]\NP))">
      <lf start="3" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="I-DAT" cat="(((S\NP)\(S\NP))/((S\NP)\(S\NP)))/NP" />
      <rule type="lex" cat="NP">
       <lf start="4" span="1" word="1988" lemma="1988" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="5" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="I-DAT" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="6" span="1" word="1992" lemma="1992" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="1" span="1" word="owned" lemma="own" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="APCOM" lemma="APCOM" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="3" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="lex" cat="NP">
      <lf start="4" span="1" word="1990" lemma="1990" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
