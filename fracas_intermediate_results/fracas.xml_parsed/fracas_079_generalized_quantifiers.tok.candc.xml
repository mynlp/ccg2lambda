<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="fa" cat="S[X]/S[X]">
    <lf start="0" span="1" word="At" lemma="at" pos="IN" chunk="I-NP" entity="O" cat="(S/S)/(S[asup]\NP)" />
    <lf start="1" span="1" word="least" lemma="least" pos="JJS" chunk="I-NP" entity="O" cat="S[asup]\NP" />
   </rule>
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="three" lemma="three" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="3" span="1" word="commissioners" lemma="commissioner" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="4" span="1" word="spend" lemma="spend" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="5" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="7" span="1" word="home" lemma="home" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="At" lemma="at" pos="IN" chunk="I-NP" entity="O" cat="(S/S)/(S[asup]\NP)" />
   <lf start="1" span="1" word="least" lemma="least" pos="JJS" chunk="I-NP" entity="O" cat="S[asup]\NP" />
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="three" lemma="three" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="male" lemma="male" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="4" span="1" word="commissioners" lemma="commissioner" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="5" span="1" word="spend" lemma="spend" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="6" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="7" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="8" span="1" word="home" lemma="home" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
</candc>
