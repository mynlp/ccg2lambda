<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fc" cat="S[dcl]/(S[adj]\NP)">
  <rule type="tr" cat="S[X]/(S[X]\NP)">
   <rule type="ba" cat="NP">
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="0" span="1" word="No" lemma="no" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="1" span="1" word="one" lemma="one" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="2" span="1" word="who" lemma="who" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
      <lf start="3" span="1" word="starts" lemma="start" pos="VBZ" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     </rule>
    </rule>
    <rule type="lex" cat="NP\NP">
     <rule type="fc" cat="S[dcl]/NP">
      <rule type="tr" cat="S[X]/(S[X]\NP)">
       <rule type="lex" cat="NP">
        <lf start="4" span="1" word="gambling" lemma="gamble" pos="VBG" chunk="I-VP" entity="O" cat="N" />
       </rule>
      </rule>
      <rule type="fc" cat="(S[dcl]\NP)/NP">
       <lf start="5" span="1" word="seriously" lemma="seriously" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
       <rule type="fa" cat="(S[dcl]\NP)/NP">
        <lf start="6" span="1" word="stops" lemma="stop" pos="VBZ" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/NP)/PP" />
        <rule type="fa" cat="PP">
         <lf start="7" span="1" word="until" lemma="until" pos="IN" chunk="I-SBAR" entity="O" cat="PP/NP" />
         <lf start="8" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="bx" cat="(S[dcl]\NP)/(S[adj]\NP)">
   <lf start="9" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
   <rule type="rp" cat="(S[X]\NP)\(S[X]\NP)">
    <lf start="10" span="1" word="broke" lemma="break" pos="VBN" chunk="I-VP" entity="O" cat="(S\NP)\(S\NP)" />
    <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="fc" cat="S[dcl]/(S[adj]\NP)">
  <rule type="tr" cat="S[X]/(S[X]\NP)">
   <rule type="ba" cat="NP">
    <rule type="ba" cat="NP">
     <lf start="0" span="1" word="Everyone" lemma="everyone" pos="DT" chunk="I-NP" entity="O" cat="NP" />
     <rule type="fa" cat="NP\NP">
      <lf start="1" span="1" word="who" lemma="who" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="2" span="1" word="starts" lemma="start" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
       <lf start="3" span="1" word="gambling" lemma="gamble" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
      </rule>
     </rule>
    </rule>
    <rule type="lex" cat="NP\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="4" span="1" word="seriously" lemma="seriously" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="5" span="1" word="continues" lemma="continue" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
       <rule type="fa" cat="PP">
        <lf start="6" span="1" word="until" lemma="until" pos="IN" chunk="I-SBAR" entity="O" cat="PP/NP" />
        <lf start="7" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="bx" cat="(S[dcl]\NP)/(S[adj]\NP)">
   <lf start="8" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
   <rule type="rp" cat="(S[X]\NP)\(S[X]\NP)">
    <lf start="9" span="1" word="broke" lemma="break" pos="VBN" chunk="I-VP" entity="O" cat="(S\NP)\(S\NP)" />
    <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
