<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="said" lemma="say" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[em]" />
    <rule type="ba" cat="S[em]">
     <rule type="fa" cat="S[em]">
      <lf start="2" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
      <rule type="ba" cat="S[dcl]">
       <rule type="lex" cat="NP">
        <lf start="3" span="1" word="Mary" lemma="Mary" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
       </rule>
       <rule type="fa" cat="S[dcl]\NP">
        <lf start="4" span="1" word="wrote" lemma="write" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="5" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="6" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="conj" cat="S[em]\S[em]">
      <lf start="7" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
      <rule type="conj" cat="S[em]\S[em]">
       <lf start="8" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="fa" cat="S[em]">
        <lf start="9" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
        <rule type="ba" cat="S[dcl]">
         <rule type="lex" cat="NP">
          <lf start="10" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
         </rule>
         <rule type="ba" cat="S[dcl]\NP">
          <lf start="11" span="1" word="did" lemma="do" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
          <lf start="12" span="1" word="too" lemma="too" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="13" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="said" lemma="say" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="Mary" lemma="Mary" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="wrote" lemma="write" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="5" span="1" word="report" lemma="report" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
