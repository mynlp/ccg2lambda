<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="ba" cat="S[ng]\NP">
     <rule type="ba" cat="S[ng]\NP">
      <rule type="ba" cat="S[ng]\NP">
       <lf start="2" span="1" word="going" lemma="go" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="3" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
        <rule type="lex" cat="NP">
         <lf start="4" span="1" word="Paris" lemma="Paris" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="5" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="ba" cat="NP[nb]">
        <rule type="lex" cat="NP">
         <lf start="6" span="1" word="car" lemma="car" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="conj" cat="NP[nb]\NP[nb]">
         <lf start="7" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
         <rule type="conj" cat="NP[nb]\NP[nb]">
          <lf start="8" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
          <rule type="fa" cat="NP[nb]">
           <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
           <lf start="10" span="1" word="students" lemma="student" pos="NNS" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="11" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="12" span="1" word="train" lemma="train" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="13" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <lf start="1" span="1" word="students" lemma="student" pos="NNS" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="2" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[ng]\NP)" />
    <rule type="ba" cat="S[ng]\NP">
     <rule type="ba" cat="S[ng]\NP">
      <lf start="3" span="1" word="going" lemma="go" pos="VBG" chunk="I-VP" entity="O" cat="S[ng]\NP" />
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="4" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="lex" cat="NP">
        <lf start="5" span="1" word="Paris" lemma="Paris" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="by" lemma="by" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="7" span="1" word="train" lemma="train" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
