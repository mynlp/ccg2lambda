<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="I-ORG" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="Ancient" lemma="Ancient" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N/N" />
     <lf start="2" span="1" word="Greeks" lemma="Greeks" pos="NNPS" chunk="I-NP" entity="I-ORG" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="were" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="4" span="1" word="all" lemma="all" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="5" span="1" word="noted" lemma="note" pos="VBN" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="6" span="1" word="philosophers" lemma="philosopher" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="Ancient" lemma="Ancient" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N/N" />
     <lf start="2" span="1" word="Greek" lemma="Greek" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="4" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="5" span="1" word="noted" lemma="noted" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="6" span="1" word="philosopher" lemma="philosopher" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
