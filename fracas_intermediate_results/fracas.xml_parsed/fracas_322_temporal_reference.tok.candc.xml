<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="Last" lemma="last" pos="JJ" chunk="I-NP" entity="I-DAT" cat="(S/S)/(S/S)" />
   <lf start="1" span="1" word="week" lemma="week" pos="NN" chunk="I-NP" entity="I-DAT" cat="S/S" />
  </rule>
  <rule type="rp" cat="S[dcl]">
   <rule type="ba" cat="S[dcl]">
    <lf start="2" span="1" word="I" lemma="I" pos="PRP" chunk="B-NP" entity="O" cat="NP" />
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="3" span="1" word="already" lemma="already" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="4" span="1" word="knew" lemma="know" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[em]" />
      <rule type="fa" cat="S[em]">
       <lf start="5" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
       <rule type="fa" cat="S[dcl]">
        <rule type="fa" cat="S[X]/S[X]">
         <lf start="6" span="1" word="when" lemma="when" pos="WRB" chunk="I-ADVP" entity="O" cat="(S/S)/S[dcl]" />
         <rule type="lp" cat="S[dcl]">
          <lf start="7" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
          <rule type="fa" cat="S[dcl]">
           <rule type="fa" cat="S[X]/S[X]">
            <lf start="8" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
            <rule type="fa" cat="NP[nb]">
             <rule type="ba" cat="NP[nb]/N">
              <rule type="fa" cat="NP[nb]">
               <lf start="9" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
               <lf start="10" span="1" word="month" lemma="month" pos="NN" chunk="I-NP" entity="O" cat="N" />
              </rule>
              <lf start="11" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
             </rule>
             <lf start="12" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
           <rule type="lp" cat="S[dcl]">
            <lf start="13" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
            <rule type="ba" cat="S[dcl]">
             <rule type="lex" cat="NP">
              <lf start="14" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
             </rule>
             <rule type="fa" cat="S[dcl]\NP">
              <lf start="15" span="1" word="would" lemma="would" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
              <rule type="fa" cat="S[b]\NP">
               <lf start="16" span="1" word="discover" lemma="discover" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/S[em]" />
               <rule type="fa" cat="S[em]">
                <lf start="17" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
                <rule type="ba" cat="S[dcl]">
                 <lf start="18" span="1" word="she" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
                 <rule type="fa" cat="S[dcl]\NP">
                  <lf start="19" span="1" word="had" lemma="have" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
                  <rule type="fa" cat="S[pt]\NP">
                   <lf start="20" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[pss]\NP)" />
                   <lf start="21" span="1" word="duped" lemma="dupe" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
                  </rule>
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
        <rule type="ba" cat="S[dcl]">
         <lf start="22" span="1" word="she" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="23" span="1" word="would" lemma="would" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
          <rule type="fa" cat="S[b]\NP">
           <lf start="24" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[adj]\NP)" />
           <lf start="25" span="1" word="furious" lemma="furious" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <lf start="26" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="ba" cat="S[dcl]">
  <lf start="0" span="1" word="It" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
  <rule type="fa" cat="S[dcl]\NP">
   <lf start="1" span="1" word="will" lemma="will" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
   <rule type="fa" cat="S[b]\NP">
    <lf start="2" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="case" lemma="case" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="5" span="1" word="that" lemma="that" pos="IN" chunk="I-ADVP" entity="O" cat="(NP\NP)/S[dcl]" />
      <rule type="fa" cat="S[dcl]">
       <rule type="fa" cat="S[X]/S[X]">
        <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(S/S)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="8" span="1" word="few" lemma="few" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="9" span="1" word="weeks" lemma="week" pos="NNS" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
       <rule type="ba" cat="S[dcl]">
        <rule type="lex" cat="NP">
         <lf start="10" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
        </rule>
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="11" span="1" word="will" lemma="will" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
         <rule type="fa" cat="S[b]\NP">
          <lf start="12" span="1" word="discover" lemma="discover" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/S[em]" />
          <rule type="fa" cat="S[em]">
           <lf start="13" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
           <rule type="rp" cat="S[dcl]">
            <rule type="ba" cat="S[dcl]">
             <rule type="ba" cat="S[dcl]">
              <lf start="14" span="1" word="she" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
              <rule type="fa" cat="S[dcl]\NP">
               <lf start="15" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
               <rule type="fa" cat="S[pt]\NP">
                <lf start="16" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[pss]\NP)" />
                <lf start="17" span="1" word="duped" lemma="dupe" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
               </rule>
              </rule>
             </rule>
             <rule type="conj" cat="S[dcl]\S[dcl]">
              <lf start="18" span="1" word=";" lemma=";" pos=";" chunk="O" entity="O" cat=";" />
              <rule type="conj" cat="S[dcl]\S[dcl]">
               <lf start="19" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
               <rule type="ba" cat="S[dcl]">
                <lf start="20" span="1" word="she" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
                <rule type="fa" cat="S[dcl]\NP">
                 <lf start="21" span="1" word="will" lemma="will" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
                 <rule type="fa" cat="S[b]\NP">
                  <lf start="22" span="1" word="be" lemma="be" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[adj]\NP)" />
                  <lf start="23" span="1" word="furious" lemma="furious" pos="JJ" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
                 </rule>
                </rule>
               </rule>
              </rule>
             </rule>
            </rule>
            <lf start="24" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
