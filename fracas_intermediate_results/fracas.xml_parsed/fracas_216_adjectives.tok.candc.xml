<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="fatter" lemma="fatter" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="4" span="1" word="politician" lemma="politician" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="5" span="1" word="than" lemma="than" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="6" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="John" lemma="John" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[adj]\NP)" />
    <rule type="ba" cat="S[adj]\NP">
     <lf start="2" span="1" word="fatter" lemma="fatter" pos="JJR" chunk="I-ADJP" entity="O" cat="S[adj]\NP" />
     <rule type="fa" cat="(S[adj]\NP)\(S[adj]\NP)">
      <lf start="3" span="1" word="than" lemma="than" pos="IN" chunk="I-PP" entity="O" cat="((S[adj]\NP)\(S[adj]\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="4" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
