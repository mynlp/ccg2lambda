<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="ba" cat="S[dcl]\NP">
     <lf start="1" span="1" word="graduated" lemma="graduate" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="2" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="3" span="1" word="March" lemma="March" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
     <lf start="4" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="5" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
      <rule type="fa" cat="S[pt]\NP">
       <lf start="6" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[pss]\NP)" />
       <rule type="ba" cat="S[pss]\NP">
        <rule type="ba" cat="S[pss]\NP">
         <lf start="7" span="1" word="employed" lemma="employ" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
         <lf start="8" span="1" word="ever" lemma="ever" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
        </rule>
        <lf start="9" span="1" word="since" lemma="since" pos="IN" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
    <rule type="fa" cat="S[pt]\NP">
     <lf start="2" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[pss]\NP)" />
     <rule type="ba" cat="S[pss]\NP">
      <lf start="3" span="1" word="unemployed" lemma="unemploy" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="4" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="6" span="1" word="past" lemma="past" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
    <rule type="ba" cat="S[pss]\NP">
     <rule type="ba" cat="S[pss]\NP">
      <lf start="2" span="1" word="unemployed" lemma="unemploy" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="3" span="1" word="at" lemma="at" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="4" span="1" word="some" lemma="some" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="5" span="1" word="time" lemma="time" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="before" lemma="before" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
      <rule type="ba" cat="S[dcl]">
       <lf start="7" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
       <lf start="8" span="1" word="graduated" lemma="graduate" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
