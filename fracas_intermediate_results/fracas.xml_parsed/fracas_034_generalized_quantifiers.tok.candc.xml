<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[b]">
  <rule type="ba" cat="S[b]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="Every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="European" lemma="european" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <lf start="2" span="1" word="can" lemma="can" pos="MD" chunk="I-VP" entity="O" cat="N" />
    </rule>
   </rule>
   <rule type="ba" cat="S[b]\NP">
    <rule type="ba" cat="S[b]\NP">
     <lf start="3" span="1" word="travel" lemma="travel" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
     <lf start="4" span="1" word="freely" lemma="freely" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="5" span="1" word="within" lemma="within" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="lex" cat="NP">
      <lf start="6" span="1" word="Europe" lemma="Europe" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>

<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="Every" lemma="every" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="person" lemma="person" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="who" lemma="who" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]\NP)" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="ba" cat="N">
        <lf start="5" span="1" word="right" lemma="right" pos="NN" chunk="I-NP" entity="O" cat="N" />
        <rule type="lex" cat="N\N">
         <rule type="fa" cat="S[to]\NP">
          <lf start="6" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
          <rule type="ba" cat="S[b]\NP">
           <lf start="7" span="1" word="live" lemma="live" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
           <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
            <lf start="8" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
            <rule type="lex" cat="NP">
             <lf start="9" span="1" word="Europe" lemma="Europe" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="10" span="1" word="can" lemma="can" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
    <rule type="ba" cat="S[b]\NP">
     <rule type="ba" cat="S[b]\NP">
      <lf start="11" span="1" word="travel" lemma="travel" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
      <lf start="12" span="1" word="freely" lemma="freely" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="13" span="1" word="within" lemma="within" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="14" span="1" word="Europe" lemma="Europe" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="15" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>

</candc>
