<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="ba" cat="S[dcl]\NP">
     <lf start="1" span="1" word="arrived" lemma="arrive" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="2" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="lex" cat="NP">
       <lf start="3" span="1" word="Paris" lemma="Paris" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="4" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="ba" cat="NP">
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="I-DAT" cat="NP[nb]/N" />
        <lf start="6" span="1" word="5th" lemma="5th" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="7" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="I-DAT" cat="(NP\NP)/NP" />
        <rule type="lex" cat="NP">
         <lf start="8" span="1" word="May" lemma="May" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="conj" cat="NP\NP">
       <lf start="9" span="1" word="," lemma="," pos="," chunk="O" entity="I-DAT" cat="," />
       <rule type="lex" cat="NP">
        <lf start="10" span="1" word="1995" lemma="1995" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Today" lemma="today" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="15th" lemma="15th" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="I-DAT" cat="(NP\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="lex" cat="NP">
        <lf start="5" span="1" word="May" lemma="May" pos="NNP" chunk="I-NP" entity="I-DAT" cat="N" />
       </rule>
       <rule type="conj" cat="NP\NP">
        <lf start="6" span="1" word="," lemma="," pos="," chunk="O" entity="I-DAT" cat="," />
        <rule type="lex" cat="NP">
         <lf start="7" span="1" word="1995" lemma="1995" pos="CD" chunk="I-NP" entity="I-DAT" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <lf start="0" span="1" word="She" lemma="she" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="bx" cat="(S[dcl]\NP)/PP">
     <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
     <lf start="2" span="1" word="still" lemma="still" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
    </rule>
    <rule type="fa" cat="PP">
     <lf start="3" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="lex" cat="NP">
      <lf start="4" span="1" word="Paris" lemma="Paris" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="1" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="2" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="lex" cat="NP">
       <lf start="3" span="1" word="Paris" lemma="Paris" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
