      <lf start="4" span="1" word="left" lemma="leave" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="5" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
