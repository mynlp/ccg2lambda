<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[b]">
  <rule type="ba" cat="S[b]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="Helen" lemma="Helen" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
    <rule type="lex" cat="NP\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="1" span="1" word="saw" lemma="see" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="ba" cat="NP">
       <rule type="fa" cat="NP[nb]">
        <lf start="2" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="3" span="1" word="chairman" lemma="chairman" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
       <rule type="fa" cat="NP\NP">
        <lf start="4" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="6" span="1" word="department" lemma="department" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[b]\NP">
    <lf start="7" span="1" word="answer" lemma="answer" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="8" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="9" span="1" word="phone" lemma="phone" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="fa" cat="NP[nb]">
     <lf start="0" span="1" word="The" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="1" span="1" word="chairman" lemma="chairman" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="4" span="1" word="department" lemma="department" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="5" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="6" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="7" span="1" word="person" lemma="person" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[b]">
  <rule type="ba" cat="S[b]">
   <rule type="ba" cat="NP">
    <lf start="0" span="1" word="There" lemma="there" pos="EX" chunk="I-NP" entity="O" cat="NP[thr]" />
    <rule type="lex" cat="NP\NP">
     <rule type="fa" cat="S[dcl]\NP[thr]">
      <lf start="1" span="1" word="is" lemma="be" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP[thr])/NP" />
      <rule type="ba" cat="NP">
       <lf start="2" span="1" word="someone" lemma="someone" pos="DT" chunk="I-NP" entity="O" cat="NP" />
       <rule type="fa" cat="NP\NP">
        <lf start="3" span="1" word="whom" lemma="whom" pos="WP" chunk="B-NP" entity="O" cat="(NP\NP)/(S[dcl]/NP)" />
        <rule type="fc" cat="S[dcl]/NP">
         <rule type="tr" cat="S[X]/(S[X]\NP)">
          <rule type="lex" cat="NP">
           <lf start="4" span="1" word="Helen" lemma="Helen" pos="NNP" chunk="B-NP" entity="I-PER" cat="N" />
          </rule>
         </rule>
         <lf start="5" span="1" word="saw" lemma="see" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[b]\NP">
    <lf start="6" span="1" word="answer" lemma="answer" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="8" span="1" word="phone" lemma="phone" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
