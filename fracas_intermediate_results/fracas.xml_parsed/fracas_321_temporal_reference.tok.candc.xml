<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
    <rule type="fa" cat="S[pt]\NP">
     <lf start="2" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[to]\NP)" />
     <rule type="fa" cat="S[to]\NP">
      <lf start="3" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
      <rule type="ba" cat="S[b]\NP">
       <rule type="ba" cat="S[b]\NP">
        <lf start="4" span="1" word="Florence" lemma="florence" pos="VB" chunk="I-NP" entity="O" cat="S[b]\NP" />
        <lf start="5" span="1" word="twice" lemma="twice" pos="RB" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
       </rule>
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="I-DAT" cat="NP[nb]/N" />
         <lf start="8" span="1" word="past" lemma="past" pos="NN" chunk="I-NP" entity="I-DAT" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="will" lemma="will" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
    <rule type="ba" cat="S[b]\NP">
     <rule type="ba" cat="S[b]\NP">
      <rule type="ba" cat="S[b]\NP">
       <lf start="2" span="1" word="go" lemma="go" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="3" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
        <rule type="lex" cat="NP">
         <lf start="4" span="1" word="Florence" lemma="Florence" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
        </rule>
       </rule>
      </rule>
      <lf start="5" span="1" word="twice" lemma="twice" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
     </rule>
     <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
      <lf start="6" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="7" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="8" span="1" word="coming" lemma="coming" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="9" span="1" word="year" lemma="year" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="10" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="0" span="1" word="Two" lemma="two" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="1" span="1" word="years" lemma="year" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
    <rule type="fa" cat="NP\NP">
     <lf start="2" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
     <rule type="lex" cat="NP">
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="now" lemma="now" pos="RB" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="4" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="5" span="1" word="will" lemma="will" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
    <rule type="fa" cat="S[b]\NP">
     <lf start="6" span="1" word="have" lemma="have" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[pt]\NP)" />
     <rule type="fa" cat="S[pt]\NP">
      <lf start="7" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[to]\NP)" />
      <rule type="fa" cat="S[to]\NP">
       <lf start="8" span="1" word="to" lemma="to" pos="TO" chunk="I-VP" entity="O" cat="(S[to]\NP)/(S[b]\NP)" />
       <rule type="ba" cat="S[b]\NP">
        <lf start="9" span="1" word="Florence" lemma="florence" pos="VB" chunk="I-NP" entity="O" cat="S[b]\NP" />
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <rule type="fa" cat="((S[X]\NP)\(S[X]\NP))/((S[X]\NP)\(S[X]\NP))">
          <rule type="fa" cat="(((S[X]\NP)\(S[X]\NP))/((S[X]\NP)\(S[X]\NP)))/(((S[X]\NP)\(S[X]\NP))/((S[X]\NP)\(S[X]\NP)))">
           <lf start="10" span="1" word="at" lemma="at" pos="IN" chunk="I-NP" entity="O" cat="((((S\NP)\(S\NP))/((S\NP)\(S\NP)))/(((S\NP)\(S\NP))/((S\NP)\(S\NP))))/(S[asup]\NP)" />
           <lf start="11" span="1" word="least" lemma="least" pos="JJS" chunk="I-NP" entity="O" cat="S[asup]\NP" />
          </rule>
          <lf start="12" span="1" word="four" lemma="four" pos="CD" chunk="I-NP" entity="O" cat="((S\NP)\(S\NP))/((S\NP)\(S\NP))" />
         </rule>
         <lf start="13" span="1" word="times" lemma="time" pos="NNS" chunk="I-NP" entity="O" cat="(S\NP)\(S\NP)" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="14" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
