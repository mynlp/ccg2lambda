<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="claimed" lemma="claim" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <rule type="ba" cat="S[dcl]">
      <lf start="2" span="1" word="he" lemma="he" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
      <rule type="fa" cat="S[dcl]\NP">
       <lf start="3" span="1" word="had" lemma="have" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
       <rule type="fa" cat="S[pt]\NP">
        <lf start="4" span="1" word="costed" lemma="cost" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/NP" />
        <rule type="fa" cat="NP[nb]">
         <lf start="5" span="1" word="his" lemma="his" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="6" span="1" word="proposal" lemma="proposal" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
     <rule type="conj" cat="S[dcl]\S[dcl]">
      <lf start="7" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="fa" cat="S[dcl]">
       <lf start="8" span="1" word="so" lemma="so" pos="RB" chunk="I-VP" entity="O" cat="S[dcl]/S[inv]" />
       <rule type="fa" cat="S[inv]">
        <lf start="9" span="1" word="did" lemma="do" pos="VBD" chunk="I-VP" entity="O" cat="S[inv]/NP" />
        <rule type="lex" cat="NP">
         <lf start="10" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="11" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="claimed" lemma="claim" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="3" span="1" word="had" lemma="have" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
      <rule type="fa" cat="S[pt]\NP">
       <lf start="4" span="1" word="costed" lemma="cost" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <rule type="ba" cat="NP[nb]/N">
         <rule type="lex" cat="NP">
          <lf start="5" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
         </rule>
         <lf start="6" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
        </rule>
        <lf start="7" span="1" word="proposal" lemma="proposal" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
