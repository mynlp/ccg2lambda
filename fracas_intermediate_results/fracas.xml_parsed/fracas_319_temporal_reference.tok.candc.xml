<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="Before" lemma="before" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <rule type="lex" cat="NP">
     <lf start="1" span="1" word="APCOM" lemma="APCOM" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="bought" lemma="buy" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="fa" cat="NP[nb]">
      <lf start="3" span="1" word="its" lemma="its" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <rule type="fa" cat="N">
       <lf start="4" span="1" word="present" lemma="present" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="office" lemma="office" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
        <lf start="6" span="1" word="building" lemma="building" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="lp" cat="S[dcl]">
   <lf start="7" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <lf start="8" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="9" span="1" word="had" lemma="have" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
      <rule type="fa" cat="S[pt]\NP">
       <lf start="10" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[ng]\NP)" />
       <rule type="ba" cat="S[ng]\NP">
        <rule type="fa" cat="S[ng]\NP">
         <lf start="11" span="1" word="paying" lemma="pay" pos="VBG" chunk="I-VP" entity="O" cat="(S[ng]\NP)/NP" />
         <rule type="ba" cat="NP">
          <rule type="lex" cat="NP">
           <rule type="fa" cat="N">
            <lf start="12" span="1" word="mortgage" lemma="mortgage" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
            <lf start="13" span="1" word="interest" lemma="interest" pos="NN" chunk="I-NP" entity="O" cat="N" />
           </rule>
          </rule>
          <rule type="fa" cat="NP\NP">
           <lf start="14" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
           <rule type="fa" cat="NP[nb]">
            <lf start="15" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
            <rule type="fa" cat="N">
             <lf start="16" span="1" word="previous" lemma="previous" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
             <lf start="17" span="1" word="one" lemma="one" pos="CD" chunk="I-NP" entity="O" cat="N" />
            </rule>
           </rule>
          </rule>
         </rule>
        </rule>
        <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
         <lf start="18" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
         <rule type="lex" cat="NP">
          <rule type="fa" cat="N">
           <lf start="19" span="1" word="8" lemma="8" pos="CD" chunk="I-NP" entity="I-DAT" cat="N/N" />
           <lf start="20" span="1" word="years" lemma="year" pos="NNS" chunk="I-NP" entity="I-DAT" cat="N" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="21" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="fa" cat="S[X]/S[X]">
    <lf start="0" span="1" word="Since" lemma="since" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
    <rule type="ba" cat="S[dcl]">
     <rule type="lex" cat="NP">
      <lf start="1" span="1" word="APCOM" lemma="APCOM" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
     </rule>
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="2" span="1" word="bought" lemma="buy" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="its" lemma="its" pos="PRP$" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <rule type="fa" cat="N">
        <lf start="4" span="1" word="present" lemma="present" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
        <rule type="fa" cat="N">
         <lf start="5" span="1" word="office" lemma="office" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="6" span="1" word="building" lemma="building" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="ba" cat="S[dcl]">
    <lf start="7" span="1" word="it" lemma="it" pos="PRP" chunk="B-NP" entity="O" cat="NP" />
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="8" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
     <rule type="fa" cat="S[pt]\NP">
      <lf start="9" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[ng]\NP)" />
      <rule type="ba" cat="S[ng]\NP">
       <rule type="fa" cat="S[ng]\NP">
        <rule type="fa" cat="(S[ng]\NP)/PP">
         <lf start="10" span="1" word="paying" lemma="pay" pos="VBG" chunk="I-VP" entity="O" cat="((S[ng]\NP)/PP)/NP" />
         <rule type="lex" cat="NP">
          <rule type="fa" cat="N">
           <lf start="11" span="1" word="mortgage" lemma="mortgage" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
           <lf start="12" span="1" word="interest" lemma="interest" pos="NN" chunk="I-NP" entity="O" cat="N" />
          </rule>
         </rule>
        </rule>
        <rule type="fa" cat="PP">
         <lf start="13" span="1" word="on" lemma="on" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
         <lf start="14" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
        </rule>
       </rule>
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="15" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
        <rule type="lex" cat="NP">
         <rule type="fa" cat="N">
          <rule type="fa" cat="N/N">
           <rule type="ba" cat="(N/N)/(N/N)">
            <lf start="16" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="O" cat="S[adj]\NP" />
            <lf start="17" span="1" word="than" lemma="than" pos="IN" chunk="I-NP" entity="O" cat="((N/N)/(N/N))\(S[adj]\NP)" />
           </rule>
           <lf start="18" span="1" word="10" lemma="10" pos="CD" chunk="I-NP" entity="I-DAT" cat="N/N" />
          </rule>
          <lf start="19" span="1" word="years" lemma="year" pos="NNS" chunk="I-NP" entity="I-DAT" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="20" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="APCOM" lemma="APCOM" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pt]\NP)" />
    <rule type="fa" cat="S[pt]\NP">
     <lf start="2" span="1" word="been" lemma="be" pos="VBN" chunk="I-VP" entity="O" cat="(S[pt]\NP)/(S[ng]\NP)" />
     <rule type="fa" cat="S[ng]\NP">
      <rule type="fa" cat="(S[ng]\NP)/PP">
       <lf start="3" span="1" word="paying" lemma="pay" pos="VBG" chunk="I-VP" entity="O" cat="((S[ng]\NP)/PP)/NP" />
       <rule type="lex" cat="NP">
        <rule type="fa" cat="N">
         <lf start="4" span="1" word="mortgage" lemma="mortgage" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="5" span="1" word="interest" lemma="interest" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
      <rule type="fa" cat="PP">
       <lf start="6" span="1" word="for" lemma="for" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
       <rule type="ba" cat="NP">
        <rule type="fa" cat="NP[nb]">
         <lf start="7" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <lf start="8" span="1" word="total" lemma="total" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="9" span="1" word="of" lemma="of" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="ba" cat="NP">
          <rule type="lex" cat="NP">
           <rule type="fa" cat="N">
            <lf start="10" span="1" word="15" lemma="15" pos="CD" chunk="I-NP" entity="I-DAT" cat="N/N" />
            <lf start="11" span="1" word="years" lemma="year" pos="NNS" chunk="I-NP" entity="I-DAT" cat="N" />
           </rule>
          </rule>
          <rule type="fa" cat="NP\NP">
           <lf start="12" span="1" word="or" lemma="or" pos="CC" chunk="I-NP" entity="I-DAT" cat="(NP\NP)/(NP\NP)" />
           <lf start="13" span="1" word="more" lemma="more" pos="JJR" chunk="I-NP" entity="I-DAT" cat="NP\NP" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="14" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
