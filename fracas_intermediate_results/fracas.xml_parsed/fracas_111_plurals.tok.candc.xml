<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="signed" lemma="sign" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="one" lemma="one" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="3" span="1" word="contract" lemma="contract" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="signed" lemma="sign" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="fa" cat="NP[nb]">
     <lf start="2" span="1" word="another" lemma="another" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <lf start="3" span="1" word="contract" lemma="contract" pos="NN" chunk="I-NP" entity="O" cat="N" />
    </rule>
   </rule>
  </rule>
  <lf start="4" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="ba" cat="NP">
    <rule type="lex" cat="NP">
     <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
    </rule>
    <rule type="conj" cat="NP\NP">
     <lf start="1" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="I-PER" cat="conj" />
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="3" span="1" word="signed" lemma="sign" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="lex" cat="NP">
     <rule type="fa" cat="N">
      <lf start="4" span="1" word="two" lemma="two" pos="CD" chunk="I-NP" entity="O" cat="N/N" />
      <lf start="5" span="1" word="contracts" lemma="contract" pos="NNS" chunk="I-NP" entity="O" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
