<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
    <rule type="ba" cat="NP">
     <rule type="fa" cat="NP[nb]">
      <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
      <lf start="3" span="1" word="factory" lemma="factory" pos="NN" chunk="I-NP" entity="O" cat="N" />
     </rule>
     <rule type="fa" cat="NP\NP">
      <lf start="4" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
      <rule type="lex" cat="NP">
       <lf start="5" span="1" word="Birmingham" lemma="Birmingham" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="6" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="ITEL" lemma="ITEL" pos="NNP" chunk="I-NP" entity="I-ORG" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="currently" lemma="currently" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)/(S\NP)" />
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="has" lemma="have" pos="VBZ" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <lf start="3" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="4" span="1" word="factory" lemma="factory" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
      <rule type="fa" cat="NP\NP">
       <lf start="5" span="1" word="in" lemma="in" pos="IN" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
       <rule type="lex" cat="NP">
        <lf start="6" span="1" word="Birmingham" lemma="Birmingham" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="7" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
