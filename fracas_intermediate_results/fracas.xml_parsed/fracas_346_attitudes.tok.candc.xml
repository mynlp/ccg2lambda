<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <rule type="fa" cat="(S[dcl]\NP)/(S[b]\NP)">
     <lf start="1" span="1" word="saw" lemma="see" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/(S[b]\NP))/NP" />
     <rule type="lex" cat="NP">
      <lf start="2" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
    </rule>
    <rule type="ba" cat="S[b]\NP">
     <rule type="fa" cat="S[b]\NP">
      <lf start="3" span="1" word="sign" lemma="sign" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
      <rule type="fa" cat="NP[nb]">
       <lf start="4" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
       <lf start="5" span="1" word="contract" lemma="contract" pos="NN" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
     <rule type="conj" cat="(S[b]\NP)\(S[b]\NP)">
      <lf start="6" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="fa" cat="S[b]\NP">
       <rule type="bx" cat="(S[b]\NP)/NP">
        <lf start="7" span="1" word="cross" lemma="cross" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
        <lf start="8" span="1" word="out" lemma="out" pos="RP" chunk="I-PRT" entity="O" cat="(S\NP)\(S\NP)" />
       </rule>
       <rule type="fa" cat="NP[nb]">
        <lf start="9" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <rule type="fa" cat="N">
         <lf start="10" span="1" word="crucial" lemma="crucial" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
         <lf start="11" span="1" word="clause" lemma="clause" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="12" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Smith" lemma="Smith" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="1" span="1" word="either" lemma="either" pos="CC" chunk="O" entity="O" cat="(S\NP)/(S\NP)" />
    <rule type="ba" cat="S[dcl]\NP">
     <rule type="fa" cat="S[dcl]\NP">
      <rule type="fa" cat="(S[dcl]\NP)/(S[b]\NP)">
       <lf start="2" span="1" word="saw" lemma="see" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/(S[b]\NP))/NP" />
       <rule type="lex" cat="NP">
        <lf start="3" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
       </rule>
      </rule>
      <rule type="fa" cat="S[b]\NP">
       <lf start="4" span="1" word="sign" lemma="sign" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
       <rule type="fa" cat="NP[nb]">
        <lf start="5" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
        <lf start="6" span="1" word="contract" lemma="contract" pos="NN" chunk="I-NP" entity="O" cat="N" />
       </rule>
      </rule>
     </rule>
     <rule type="conj" cat="(S[dcl]\NP)\(S[dcl]\NP)">
      <lf start="7" span="1" word="or" lemma="or" pos="CC" chunk="O" entity="O" cat="conj" />
      <rule type="fa" cat="S[dcl]\NP">
       <rule type="fa" cat="(S[dcl]\NP)/(S[b]\NP)">
        <lf start="8" span="1" word="saw" lemma="see" pos="VBD" chunk="I-VP" entity="O" cat="((S[dcl]\NP)/(S[b]\NP))/NP" />
        <rule type="lex" cat="NP">
         <lf start="9" span="1" word="Jones" lemma="Jones" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
        </rule>
       </rule>
       <rule type="fa" cat="S[b]\NP">
        <rule type="bx" cat="(S[b]\NP)/NP">
         <lf start="10" span="1" word="cross" lemma="cross" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/NP" />
         <lf start="11" span="1" word="out" lemma="out" pos="RP" chunk="I-PRT" entity="O" cat="(S\NP)\(S\NP)" />
        </rule>
        <rule type="fa" cat="NP[nb]">
         <lf start="12" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
         <rule type="fa" cat="N">
          <lf start="13" span="1" word="crucial" lemma="crucial" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
          <lf start="14" span="1" word="clause" lemma="clause" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="15" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
