<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="lex" cat="NP">
    <lf start="0" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="O" cat="N" />
   </rule>
   <rule type="ba" cat="S[dcl]\NP">
    <lf start="1" span="1" word="suggested" lemma="suggest" pos="VBD" chunk="I-VP" entity="O" cat="S[dcl]\NP" />
    <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
     <lf start="2" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
     <rule type="ba" cat="NP">
      <rule type="fa" cat="NP[nb]">
       <rule type="ba" cat="NP[nb]/N">
        <rule type="lex" cat="NP">
         <lf start="3" span="1" word="Frank" lemma="Frank" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
        </rule>
        <lf start="4" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
       </rule>
       <rule type="fa" cat="N">
        <lf start="5" span="1" word="boss" lemma="boss" pos="NN" chunk="I-NP" entity="O" cat="N/S[em]" />
        <rule type="fa" cat="S[em]">
         <lf start="6" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="S[em]/S[dcl]" />
         <rule type="ba" cat="S[dcl]">
          <lf start="7" span="1" word="they" lemma="they" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
          <rule type="rp" cat="S[dcl]\NP">
           <rule type="fa" cat="S[dcl]\NP">
            <lf start="8" span="1" word="should" lemma="should" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
            <rule type="ba" cat="S[b]\NP">
             <rule type="ba" cat="S[b]\NP">
              <lf start="9" span="1" word="go" lemma="go" pos="VB" chunk="I-VP" entity="O" cat="S[b]\NP" />
              <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
               <lf start="10" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="((S\NP)\(S\NP))/NP" />
               <rule type="fa" cat="NP[nb]">
                <lf start="11" span="1" word="the" lemma="the" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
                <lf start="12" span="1" word="meeting" lemma="meeting" pos="NN" chunk="I-NP" entity="O" cat="N" />
               </rule>
              </rule>
             </rule>
             <lf start="13" span="1" word="together" lemma="together" pos="RB" chunk="I-ADVP" entity="O" cat="(S\NP)\(S\NP)" />
            </rule>
           </rule>
           <lf start="14" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
      <rule type="conj" cat="NP\NP">
       <lf start="15" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="O" cat="conj" />
       <rule type="ba" cat="NP">
        <rule type="lex" cat="NP">
         <lf start="16" span="1" word="Carl" lemma="Carl" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
        </rule>
        <rule type="fa" cat="NP\NP">
         <lf start="17" span="1" word="to" lemma="to" pos="TO" chunk="I-PP" entity="O" cat="(NP\NP)/NP" />
         <rule type="fa" cat="NP[nb]">
          <rule type="ba" cat="NP[nb]/N">
           <rule type="lex" cat="NP">
            <lf start="18" span="1" word="Alan" lemma="Alan" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
           </rule>
           <lf start="19" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
          </rule>
          <lf start="20" span="1" word="wife" lemma="wife" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="21" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="fa" cat="S[dcl]">
  <rule type="fa" cat="S[X]/S[X]">
   <lf start="0" span="1" word="If" lemma="if" pos="IN" chunk="I-SBAR" entity="O" cat="(S/S)/S[dcl]" />
   <rule type="ba" cat="S[dcl]">
    <lf start="1" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="2" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
     <rule type="ba" cat="S[pss]\NP">
      <lf start="3" span="1" word="suggested" lemma="suggest" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
      <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
       <lf start="4" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
       <rule type="ba" cat="S[dcl]">
        <rule type="fa" cat="NP[nb]">
         <rule type="ba" cat="NP[nb]/N">
          <rule type="ba" cat="NP">
           <rule type="lex" cat="NP">
            <lf start="5" span="1" word="Bill" lemma="Bill" pos="NNP" chunk="I-NP" entity="O" cat="N" />
           </rule>
           <rule type="conj" cat="NP\NP">
            <lf start="6" span="1" word="and" lemma="and" pos="CC" chunk="I-NP" entity="O" cat="conj" />
            <rule type="lex" cat="NP">
             <lf start="7" span="1" word="Frank" lemma="Frank" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
            </rule>
           </rule>
          </rule>
          <lf start="8" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
         </rule>
         <lf start="9" span="1" word="boss" lemma="boss" pos="NN" chunk="I-NP" entity="O" cat="N" />
        </rule>
        <rule type="fa" cat="S[dcl]\NP">
         <lf start="10" span="1" word="should" lemma="should" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
         <rule type="fa" cat="S[b]\NP">
          <lf start="11" span="1" word="go" lemma="go" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[adj]\NP)" />
          <lf start="12" span="1" word="together" lemma="together" pos="RB" chunk="I-ADVP" entity="O" cat="S[adj]\NP" />
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <rule type="lp" cat="S[dcl]">
   <lf start="13" span="1" word="," lemma="," pos="," chunk="O" entity="O" cat="," />
   <rule type="rp" cat="S[dcl]">
    <rule type="ba" cat="S[dcl]">
     <lf start="14" span="1" word="it" lemma="it" pos="PRP" chunk="I-NP" entity="O" cat="NP" />
     <rule type="fa" cat="S[dcl]\NP">
      <lf start="15" span="1" word="was" lemma="be" pos="VBD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[pss]\NP)" />
      <rule type="ba" cat="S[pss]\NP">
       <lf start="16" span="1" word="suggested" lemma="suggest" pos="VBN" chunk="I-VP" entity="O" cat="S[pss]\NP" />
       <rule type="fa" cat="(S[X]\NP)\(S[X]\NP)">
        <lf start="17" span="1" word="that" lemma="that" pos="IN" chunk="I-SBAR" entity="O" cat="((S\NP)\(S\NP))/S[dcl]" />
        <rule type="ba" cat="S[dcl]">
         <rule type="fa" cat="NP[nb]">
          <rule type="ba" cat="NP[nb]/N">
           <rule type="ba" cat="NP">
            <rule type="lex" cat="NP">
             <lf start="18" span="1" word="Carl" lemma="Carl" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
            </rule>
            <rule type="conj" cat="NP\NP">
             <lf start="19" span="1" word="and" lemma="and" pos="CC" chunk="O" entity="I-PER" cat="conj" />
             <rule type="lex" cat="NP">
              <lf start="20" span="1" word="Alan" lemma="Alan" pos="NNP" chunk="I-NP" entity="I-PER" cat="N" />
             </rule>
            </rule>
           </rule>
           <lf start="21" span="1" word="'s" lemma="'s" pos="POS" chunk="B-NP" entity="O" cat="(NP[nb]/N)\NP" />
          </rule>
          <lf start="22" span="1" word="wife" lemma="wife" pos="NN" chunk="I-NP" entity="O" cat="N" />
         </rule>
         <rule type="fa" cat="S[dcl]\NP">
          <lf start="23" span="1" word="should" lemma="should" pos="MD" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/(S[b]\NP)" />
          <rule type="fa" cat="S[b]\NP">
           <lf start="24" span="1" word="go" lemma="go" pos="VB" chunk="I-VP" entity="O" cat="(S[b]\NP)/(S[adj]\NP)" />
           <lf start="25" span="1" word="together" lemma="together" pos="RB" chunk="I-ADVP" entity="O" cat="S[adj]\NP" />
          </rule>
         </rule>
        </rule>
       </rule>
      </rule>
     </rule>
    </rule>
    <lf start="26" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
   </rule>
  </rule>
 </rule>
</ccg>
</candc>
