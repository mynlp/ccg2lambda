<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="candc.xml"?>

<candc>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="ba" cat="S[dcl]">
   <rule type="fa" cat="NP[nb]">
    <lf start="0" span="1" word="A" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
    <rule type="fa" cat="N">
     <lf start="1" span="1" word="few" lemma="few" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
     <rule type="fa" cat="N">
      <lf start="2" span="1" word="female" lemma="female" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="3" span="1" word="committee" lemma="committee" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="4" span="1" word="members" lemma="member" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
   <rule type="fa" cat="S[dcl]\NP">
    <lf start="5" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
    <rule type="fa" cat="PP">
     <lf start="6" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
     <rule type="lex" cat="NP">
      <lf start="7" span="1" word="Scandinavia" lemma="Scandinavia" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="8" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
<ccg>
 <rule type="rp" cat="S[dcl]">
  <rule type="fa" cat="S[dcl]">
   <rule type="fa" cat="S[X]/S[X]">
    <lf start="0" span="1" word="At" lemma="at" pos="IN" chunk="I-ADVP" entity="O" cat="(S/S)/(S[asup]\NP)" />
    <lf start="1" span="1" word="least" lemma="least" pos="JJS" chunk="I-ADVP" entity="O" cat="S[asup]\NP" />
   </rule>
   <rule type="ba" cat="S[dcl]">
    <rule type="fa" cat="NP[nb]">
     <lf start="2" span="1" word="a" lemma="a" pos="DT" chunk="I-NP" entity="O" cat="NP[nb]/N" />
     <rule type="fa" cat="N">
      <lf start="3" span="1" word="few" lemma="few" pos="JJ" chunk="I-NP" entity="O" cat="N/N" />
      <rule type="fa" cat="N">
       <lf start="4" span="1" word="committee" lemma="committee" pos="NN" chunk="I-NP" entity="O" cat="N/N" />
       <lf start="5" span="1" word="members" lemma="member" pos="NNS" chunk="I-NP" entity="O" cat="N" />
      </rule>
     </rule>
    </rule>
    <rule type="fa" cat="S[dcl]\NP">
     <lf start="6" span="1" word="are" lemma="be" pos="VBP" chunk="I-VP" entity="O" cat="(S[dcl]\NP)/PP" />
     <rule type="fa" cat="PP">
      <lf start="7" span="1" word="from" lemma="from" pos="IN" chunk="I-PP" entity="O" cat="PP/NP" />
      <rule type="lex" cat="NP">
       <lf start="8" span="1" word="Scandinavia" lemma="Scandinavia" pos="NNP" chunk="I-NP" entity="I-LOC" cat="N" />
      </rule>
     </rule>
    </rule>
   </rule>
  </rule>
  <lf start="9" span="1" word="." lemma="." pos="." chunk="O" entity="O" cat="." />
 </rule>
</ccg>
</candc>
